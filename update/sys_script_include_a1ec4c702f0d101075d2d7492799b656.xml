        <api_name>x_58872_needit.GetEmail</api_name>
        <caller_access/>
        <client_callable>true</client_callable>
        <description>The script include to get the email address from the sys_id provided by the client-side script.</description>
        <name>GetEmail</name>
        <script><![CDATA[var GetEmail = Class.create();
GetEmail.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	
	getEmail: function() {
		var userRecord = new GlideRecord("sys_user");
		userRecord.get(this.getParameter('sysparm_userID'));
		return userRecord.email + '';
	},

    type: 'GetEmail'
});]]></script>
